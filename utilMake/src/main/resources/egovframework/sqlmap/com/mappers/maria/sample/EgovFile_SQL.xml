<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="egovframework.com.common.service.impl.EgovFileMapper">

	<select id="selectFileInfo" parameterType="egovMap" resultType="egovMap">
		<![CDATA[
			SELECT
			    STORED_FILE_NAME,
			    ORIGINAL_FILE_NAME
			FROM
			    FILE_TB
			WHERE
			    FILE_SEQ = #{fileSeq}
		]]>
	</select>
	
	<select id="selectSampleCodeList" parameterType="egovMap" resultType="egovMap">
		<![CDATA[
			SELECT
			    CODE_PARENT,
			    CODE_VALUE,
			    CODE_NAME,
			    CODE_ORDER
			FROM
			    SAMPLE_CODE
		]]>
	</select>

</mapper>